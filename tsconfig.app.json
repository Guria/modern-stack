{
	"extends": "./tsconfig.base.json",
	"compilerOptions": {
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"jsx": "react-jsx",
		"allowJs": true,
		"allowImportingTsExtensions": true,
		"composite": true,
		"baseUrl": ".",
		"paths": {
			"#styled-system/*": ["./src/shared/styled-system/*"],
			"#*": ["./src/*"]
		}
	},
	"include": ["src"]
}
